<?xml version="1.0" encoding="utf-8"?>
<Container>
  <TemplateURL>https://gitlab.kiselovdev.com/docker/unraid-templates/raw/develop/plex.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <Date>2017-08-20</Date>
  <Changes></Changes>
  <Name>plex</Name>
  <Support></Support>
  <Description>
  Plex organizes video, music and photos from personal media libraries and streams them to smart TVs, streaming boxes and mobile devices. [br]
  </Description>
  <Project>https://plex.tv/</Project>
  <Registry>https://hub.docker.com/r/gkiselov/plex/</Registry>
  <GitHub>https://github.com/gkiselov/unraid-templates</GitHub>
  <Repository>gkiselov/plex</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>Version</Name>
      <Value>0.1</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:32400]/web</WebUI>
  <Icon>https://raw.githubusercontent.com/gkiselov/unraid-templates/master/templates/img/plex-icon.png</Icon>
  <ExtraParams></ExtraParams>
  <BaseImage>ubuntu/16.04</BaseImage>
</Container>
